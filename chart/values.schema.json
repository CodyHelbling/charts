{
    "$schema": "https://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "annotations": {
            "type": "object"
        },
        "app": {
            "type": "object",
            "properties": {
                "memory": {
                    "type": "string"
                },
                "runtimeMode": {
                    "type": "string"
                }
            }
        },
        "autoscaling": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "database": {
            "type": "object",
            "properties": {
                "existingSecret": {
                    "type": "string"
                },
                "host": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "password": {
                    "type": "string"
                },
                "port": {
                    "type": "integer"
                },
                "protocol": {
                    "type": "string"
                },
                "root": {
                    "type": "object",
                    "properties": {
                        "password": {
                            "type": "string"
                        },
                        "user": {
                            "type": "string"
                        }
                    }
                },
                "tls": {
                    "type": "boolean"
                },
                "tlsMode": {
                    "type": "string"
                },
                "user": {
                    "type": "string"
                }
            }
        },
        "dnsConfig": {
            "type": "object"
        },
        "dnsPolicy": {
            "type": "string"
        },
        "environment": {
            "type": "array"
        },
        "extraContainers": {
            "type": "array"
        },
        "extraInitContainers": {
            "type": "array"
        },
        "extraVolumeMounts": {
            "type": "array"
        },
        "extraVolumes": {
            "type": "array"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                },
                "tag": {
                    "type": "string"
                }
            }
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "ingress": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                },
                "extraPaths": {
                    "type": "array"
                },
                "hosts": {
                    "type": "array"
                },
                "paths": {
                    "type": "array"
                },
                "tls": {
                    "type": "array"
                }
            }
        },
        "initContainers": {
            "type": "object",
            "properties": {
                "image": {
                    "type": "object",
                    "properties": {
                        "repository": {
                            "type": "string"
                        },
                        "tag": {
                            "type": "string"
                        }
                    }
                },
                "resources": {
                    "type": "object"
                }
            }
        },
        "kickstart": {
            "type": "object",
            "properties": {
                "data": {
                    "type": "object"
                },
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "lifecycle": {
            "type": "object"
        },
        "livenessProbe": {
            "type": "object",
            "properties": {
                "failureThreshold": {
                    "type": "integer"
                },
                "httpGet": {
                    "type": "object",
                    "properties": {
                        "path": {
                            "type": "string"
                        },
                        "port": {
                            "type": "string"
                        }
                    }
                },
                "periodSeconds": {
                    "type": "integer"
                },
                "timeoutSeconds": {
                    "type": "integer"
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podDisruptionBudget": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "readinessProbe": {
            "type": "object",
            "properties": {
                "failureThreshold": {
                    "type": "integer"
                },
                "httpGet": {
                    "type": "object",
                    "properties": {
                        "path": {
                            "type": "string"
                        },
                        "port": {
                            "type": "string"
                        }
                    }
                },
                "timeoutSeconds": {
                    "type": "integer"
                }
            }
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object"
        },
        "search": {
            "type": "object",
            "properties": {
                "engine": {
                    "type": "string"
                },
                "host": {
                    "type": "string"
                },
                "port": {
                    "type": "integer"
                },
                "protocol": {
                    "type": "string"
                }
            }
        },
        "service": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "port": {
                    "type": "integer"
                },
                "spec": {
                    "type": "object"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "startupProbe": {
            "type": "object",
            "properties": {
                "failureThreshold": {
                    "type": "integer"
                },
                "httpGet": {
                    "type": "object",
                    "properties": {
                        "path": {
                            "type": "string"
                        },
                        "port": {
                            "type": "string"
                        }
                    }
                },
                "periodSeconds": {
                    "type": "integer"
                },
                "timeoutSeconds": {
                    "type": "integer"
                }
            }
        },
        "tolerations": {
            "type": "array"
        }
    }
}
